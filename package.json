{
  "name": "nc_youtube_clone_coding",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla and NodeJS",
  "author": "Kim Seonwoo",
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.10",
    "autoprefixer": "^10.2.5",
    "aws-sdk": "^2.874.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^4.4.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.9.1",
    "css-loader": "^5.1.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.4.1",
    "mongoose": "^5.12.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "postcss": "^8.2.8",
    "postcss-loader": "^5.2.0",
    "pug": "^3.0.2",
    "shelljs": "^0.8.4",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "scss": "^0.2.4",
    "shx": "^0.3.3",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static'",
    "dev:assets": "cd src && webpack -w",
    "build:assets": "cd src && webpack ",
    "build:server": "babel src --out-dir build --ignore src/static --ignore src/assets --ignore src/webpack.config.js",
    "copyAll": "shx cp -R src/static build && shx cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
